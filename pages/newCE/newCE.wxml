<!--pages/newCE/newCE.wxml-->
<view>
  <i-row>
    <i-col offset="1" span="22" i-class="col-class">
      <view class="round-card" style="margin-top:30rpx;height:313rpx;background-color:#0879CE">
          <view>智能选科</view>
          <view>结合辽宁省考生</view>
          <view>大数据综合分析（适合高一/高二学生）</view>
          预计时长30分钟<i-button bind:click="toQuiz" type="primary" inline="true" size="small">开始制定最佳选</i-button>
      </view>
    </i-col>
  </i-row>
</view>
<view style='width: 100%;  height: 20rpx'/>
<view>
  <i-row>
    <i-col offset="1" span="10" i-class="col-class">
      <view bindtap="checkReport" class="round-card card-color-0" style="margin-top:30rpx;height:204rpx;background-color:#3EE3C3;margin-left:10rpx">专业查选科
      </view>
    </i-col>
    <i-col offset="2" span="10" i-class="col-class">
      <view bindtap="checkReport" class="round-card card-color-0" style="margin-top:30rpx;height:204rpx;background-color:#FF8060;margin-right:10rpx">大学查选科
      </view>
    </i-col>
  </i-row>
</view>
<view style='width: 100%;  height: 15rpx'/>
<view>
  <i-row>
    <i-col offset="1" span="22" i-class="col-class">
      <view bindtap="checkReport" class="round-card card-color-0" style="margin-top:30rpx;height:204rpx;background-color:#8081F7">
      科目推专业
      </view>
    </i-col>
  </i-row>
</view>